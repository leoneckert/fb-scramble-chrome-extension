{
    "name": "oamo2",
    "version": "2.0",
    "manifest_version": 2,
    "description": "oamo2",
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "permissions": [
        "tabs", 
        "http://*/*", 
        "https://*/*"
    ],
    // "content_scripts": [
    //     {
    //       "matches": [
    //         "https://www.facebook.com/"
    //       ],
    //       "js": [
    //         "jquery.js",
    //         "content_script.js"
    //       ]
    //     }
    // ],
    "content_scripts": [
        {
          "matches": ["https://www.facebook.com/*"],
          "js": ["jquery.js", "inject.js"]
        }
    ],
    "web_accessible_resources": [
        "manifest.json",
        "dict.json"
    ]
}